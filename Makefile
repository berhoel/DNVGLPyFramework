SHELL = /bin/sh

all:	build
	@echo "nothing to do"

test: build
	make -C test test

doc:
	$(MAKE) -C doc html

%_test: build
	make -C test $@

%: build
	make -C test $@

build:
	python setup.py build

.PHONY: build
.PHONY:	doc

